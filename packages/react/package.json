{
  "name": "@nearstack-dev/react",
  "version": "0.1.1",
  "description": "React bindings (useModel, useLiveQuery, AI hooks)",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./ai": {
      "import": {
        "types": "./dist/ai.d.ts",
        "default": "./dist/ai.js"
      },
      "require": {
        "types": "./dist/ai.d.ts",
        "default": "./dist/ai.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "author": "Dakota Kim",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "@nearstack-dev/ai": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@nearstack-dev/ai": {
      "optional": true
    }
  },
  "dependencies": {
    "@nearstack-dev/core": "workspace:*"
  },
  "devDependencies": {
    "@nearstack-dev/ai": "workspace:*",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.0",
    "jsdom": "^26.0.0",
    "react": "^19.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
